{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "noImplicitAny": true,
    "useDefineForClassFields": true,
    "moduleResolution": "Node",
    "module": "ESNext",
    "target": "ESNext",
    "allowJs": true,
    "strict": true,
    "jsx": "react-jsx",
    "isolatedModules": true,
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "*"
      ],
      "@/Header": [
        "components/Header/Header"
      ],
      "@/Main": [
        "components/Main"
      ],
      "@/assets": [
        "assets"
      ],
      "@/utils": [
        "utils"
      ],
      "@/api": [
        "api"
      ],
      "@/ts": [
        "ts/"
      ],
      "@/GaragePage": [
        "components/GaragePage/GaragePage"
      ],
      "@/CarControl": [
        "components/GaragePage/CarControl/CarControl"
      ],
      "@/CarSettings": [
        "components/GaragePage/CarControl/CarSettings/CarSettings"
      ],
      "@/Pagination": [
        "components/GaragePage/Pagination/Pagination"
      ],
    }
  },
  "files": [
    "src/index.tsx"
  ],
  "include": [
    "src",
    "src/**/*.ts",
    ".eslintrc.json",
    "declarations.d.ts"
  ],
}